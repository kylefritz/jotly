<header class="bar bar-nav">
  <a class="icon icon-compose pull-right" href="<%= new_jot_path %>"></a>
  <h1 class="title">Jotly</h1>
</header>

<% @jots.each do |jot| %>
  <ul class="table-view">
    <li class="table-view-cell">
      <a class="navigate-right">
        <%= jot.message %>
      </a>
      <%= link_to "", edit_jot_path(jot) %>
    </li>

<% end %>
