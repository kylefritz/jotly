<h1> Login Page </h1>

<div class="content-padded">
  <h2>Sign-In</h2>
  <%= form_tag session_path do %>
    <%= email_field_tag :email, nil, placeholder: 'tyrion@house-lannister.got' %>
    <%= submit_tag 'Sign In!', class: 'btn btn-block' %>
  <% end %>

  <a class='btn btn-block' href= "<%= new_user_path %>"> Sign Up </a>


</div>
